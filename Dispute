// Each record: [CaseID, Customer, Plant, Dept, Invoice, Amount, Reason, Status]

let currentPlant = 'All';
let sortCol = 'amt';
let sortDir = -1; // -1 = desc

// ═══════════════════════════════════════════════
// INIT
// ═══════════════════════════════════════════════
function init() {
  buildPlantFilter();
  render();
  document.getElementById('plant-filter').addEventListener('change', function(){
    currentPlant = this.value;
    render();
  });
  document.getElementById('csv-input').addEventListener('change', handleCSV);
  document.querySelector('th[data-sortable]').addEventListener('click', function(){
    sortDir *= -1;
    render();
  });
}

// ═══════════════════════════════════════════════
// FILTER
// ═══════════════════════════════════════════════
function buildPlantFilter() {
  const sel = document.getElementById('plant-filter');
  const plants = [...new Set(RAW.map(r => r[2]))].sort();
  plants.forEach(p => {
    const o = document.createElement('option');
    o.value = p; o.textContent = p;
    sel.appendChild(o);
  });
}

function filtered() {
  if (currentPlant === 'All') return RAW;
  return RAW.filter(r => r[2] === currentPlant);
}

// ═══════════════════════════════════════════════
// RENDER ALL
// ═══════════════════════════════════════════════
function render() {
  const data = filtered();
  renderKPIs(data);
  renderStatusBars(data);
  renderDonut(data);
  renderTable(data);
  document.getElementById('record-count').innerHTML = '<strong>' + data.length + '</strong> records';
}

// ═══════════════════════════════════════════════
// KPIs
// ═══════════════════════════════════════════════
function fmt(n) {
  return '$' + n.toLocaleString('en-US', {minimumFractionDigits:2, maximumFractionDigits:2});
}

function renderKPIs(data) {
  const total = data.reduce((s,r) => s + r[5], 0);
  const open = data.filter(r => r[7]==='New' || r[7]==='Being Processed');
  const openAmt = open.reduce((s,r) => s + r[5], 0);

  document.getElementById('kpi-total').textContent = fmt(total);
  document.getElementById('kpi-total-sub').textContent = currentPlant === 'All' ? 'across all plants' : currentPlant;
  document.getElementById('kpi-cases').textContent = data.length.toLocaleString();
  document.getElementById('kpi-open').textContent = open.length.toLocaleString();
  document.getElementById('kpi-avg').textContent = open.length ? fmt(openAmt / open.length) : '/bin/sh.00';
}

// ═══════════════════════════════════════════════
// STATUS BAR CHART
// ═══════════════════════════════════════════════
const STATUS_ORDER = ['New','Being Processed','Closed','Accepted','To Be Requested','Rejected','Voided'];
const STATUS_COLORS = {
  'New':'var(--accent2)',
  'Being Processed':'var(--accent)',
  'Closed':'#4b5563',
  'Accepted':'var(--accent3)',
  'To Be Requested':'var(--accent5)',
  'Rejected':'var(--accent4)',
  'Voided':'var(--accent6)'
};

function renderStatusBars(data) {
  const counts = {};
  STATUS_ORDER.forEach(s => counts[s] = 0);
  data.forEach(r => { if(counts[r[7]] !== undefined) counts[r[7]]++; });
  const max = Math.max(...Object.values(counts));
  const container = document.getElementById('status-bars');
  container.innerHTML = '';
  STATUS_ORDER.forEach(status => {
    const count = counts[status];
    if (count === 0 && currentPlant !== 'All') return;
    const pct = max > 0 ? (count / max * 100) : 0;
    container.innerHTML += `
      <div class="status-bar-row">
        <div class="status-bar-label">${status}</div>
        <div class="status-bar-track">
          <div class="status-bar-fill" style="width:${pct}%;background:${STATUS_COLORS[status]}">
            ${pct > 12 ? '<span>' + count + '</span>' : ''}
          </div>
        </div>
        <div class="status-bar-count">${pct <= 12 ? count : ''}</div>
      </div>`;
  });
}

// ═══════════════════════════════════════════════
// DONUT CHART (SVG)
// ═══════════════════════════════════════════════
const DONUT_COLORS = ['var(--accent)','var(--accent2)','var(--accent3)','var(--accent4)','var(--accent5)','var(--accent6)'];

function renderDonut(data) {
  // group by reason, sort desc by count
  const map = {};
  data.forEach(r => { map[r[6]] = (map[r[6]]||0)+1; });
  const sorted = Object.entries(map).sort((a,b)=>b[1]-a[1]);
  const top6 = sorted.slice(0,6);
  const otherCount = sorted.slice(6).reduce((s,e)=>s+e[1],0);
  const total = data.length;

  // Build SVG arcs
  const R = 60, cx = 80, cy = 80;
  const stroke = 22;
  let cumAngle = -Math.PI/2; // start top
  let svgPaths = '';
  const legendItems = [];

  const allSlices = [...top6];
  if (otherCount > 0) allSlices.push(['Other', otherCount]);

  allSlices.forEach((entry, i) => {
    const [label, count] = entry;
    const angle = (count / total) * 2 * Math.PI;
    const color = i < 6 ? DONUT_COLORS[i] : '#3a3f52';

    // arc path
    const x1 = cx + R * Math.cos(cumAngle);
    const y1 = cy + R * Math.sin(cumAngle);
    const endAngle = cumAngle + angle;
    const x2 = cx + R * Math.cos(endAngle);
    const y2 = cy + R * Math.sin(endAngle);
    const large = angle > Math.PI ? 1 : 0;

    svgPaths += `<path d="M ${x1} ${y1} A ${R} ${R} 0 ${large} 1 ${x2} ${y2}" fill="none" stroke="${color}" stroke-width="${stroke}" stroke-linecap="butt"/>`;
    cumAngle = endAngle;

    if (i < 6) {
      legendItems.push({ label, count, color });
    }
  });

  document.getElementById('donut-svg').innerHTML = svgPaths;
  document.getElementById('donut-total').textContent = total.toLocaleString();

  // Legend
  const legend = document.getElementById('donut-legend');
  legend.innerHTML = legendItems.map(item => `
    <div class="legend-item">
      <div class="legend-dot" style="background:${item.color}"></div>
      <span>${item.label}</span>
      <span class="legend-val">${item.count}</span>
    </div>`).join('');
}

// ═══════════════════════════════════════════════
// TABLE
// ═══════════════════════════════════════════════
const BADGE_MAP = {
  'New':'badge-new',
  'Being Processed':'badge-processing',
  'Closed':'badge-closed',
  'Accepted':'badge-accepted',
  'To Be Requested':'badge-requested',
  'Rejected':'badge-rejected',
  'Voided':'badge-voided'
};

function renderTable(data) {
  // sort by amount desc
  const sorted = [...data].sort((a,b) => sortDir * (a[5]-b[5]));
  const top50 = sorted.slice(0, 50);
  const tbody = document.getElementById('table-body');
  tbody.innerHTML = top50.map(r => `
    <tr>
      <td>${r[0]}</td>
      <td>${r[1]}</td>
      <td>${r[2]}</td>
      <td>${r[3]}</td>
      <td>${r[4]}</td>
      <td class="amt">${fmt(r[5])}</td>
      <td>${r[6]}</td>
      <td><span class="badge ${BADGE_MAP[r[7]]||''}">${r[7]}</span></td>
    </tr>`).join('');
  document.getElementById('table-sub').textContent = 'Showing top 50 of ' + data.length + ' disputes by amount';
}

// ═══════════════════════════════════════════════
// CSV UPLOAD
// ═══════════════════════════════════════════════
function handleCSV(e) {
  const file = e.target.files[0];
  if (!file) return;
  Papa.parse(file, {
    header: true,
    skipEmptyLines: true,
    complete: function(results) {
      try {
        const rows = results.data;
        const newData = [];
        rows.forEach(row => {
          // flexible column detection
          const caseId = parseInt(row['Case ID'] || row['case id'] || row['CaseID'] || 0);
          let customer = row['Customer'] || row['customer'] || '';
          // extract name from parens if present
          const m = customer.match(/\((.+?)\)/);
          if (m) customer = m[1];
          const plant = row['Plant Name'] || row['plant name'] || row['Plant'] || 'Unassigned';
          const dept = row['Department'] || row['department'] || row['Dept'] || '';
          const invoice = row['Invoice'] || row['invoice'] || '';
          let amt = parseFloat((row['Disputed Amount'] || row['disputed amount'] || '0').toString().replace(/[,USD\s]/g,''));
          if (isNaN(amt)) amt = 0;
          let reason = row['Reason'] || row['reason'] || 'Unknown';
          const rm = reason.match(/\((.+?)\)/);
          if (rm) reason = rm[1];
          let status = row['Status'] || row['status'] || '';
          const sm = status.match(/\((.+?)\)/);
          if (sm) status = sm[1];

          newData.push([caseId, customer.trim(), (plant||'Unassigned').trim(), dept.toString().trim(), invoice.toString().trim(), amt, reason.trim(), status.trim()]);
        });
        RAW = newData;
        currentPlant = 'All';
        // rebuild filter
        const sel = document.getElementById('plant-filter');
        sel.innerHTML = '<option value="All">All Plants</option>';
        const plants = [...new Set(RAW.map(r => r[2]))].sort();
        plants.forEach(p => {
          const o = document.createElement('option');
          o.value = p; o.textContent = p;
          sel.appendChild(o);
        });
        sel.value = 'All';
        render();
        showToast('Loaded ' + RAW.length + ' records from ' + file.name);
      } catch(err) {
        showToast('Error parsing CSV: ' + err.message);
      }
      e.target.value = '';
    }
  });
}

// ═══════════════════════════════════════════════
// TOAST
// ═══════════════════════════════════════════════
function showToast(msg) {
  const t = document.getElementById('toast');
  document.getElementById('toast-msg').textContent = msg;
  t.classList.add('show');
  setTimeout(() => t.classList.remove('show'), 3200);
}

// ─── GO ─────────────────────────────────────────
init();
</script>
</body>
</html>
